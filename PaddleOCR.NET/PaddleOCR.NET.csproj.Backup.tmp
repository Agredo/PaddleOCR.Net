<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>PaddleOCR.NET</RootNamespace>
    <AssemblyName>PaddleOCR.NET</AssemblyName>
    <Description>ONNX-based PaddleOCR implementation for .NET with Fluent API</Description>
    <PackageTags>ocr;paddleocr;onnx;text-detection;text-recognition</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ML.OnnxRuntime" Version="1.23.2" />
    <PackageReference Include="SkiaSharp" Version="3.119.1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Models\Detection\V3\" />
    <Folder Include="Models\Detection\V5\" />
    <Folder Include="Pipeline\" />
    <Folder Include="Models\Preprocessing\Orientation\Document\" />
    <Folder Include="Models\Preprocessing\Orientation\Textline\" />
    <Folder Include="Models\Preprocessing\Unwarping\" />
    <Folder Include="Models\Recognition\" />
  </ItemGroup>

</Project>
